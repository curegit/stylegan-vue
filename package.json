{
  "name": "stylegan-vue",
  "version": "1.0.0",
  "description": "StyleGAN client SPA for the StyleGAN FastAPI",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview --port 8080",
    "check": "vue-tsc --noEmit && eslint --ext .ts,.tsx,.vue . && prettier -cu --ignore-path .gitignore .",
    "lint": "eslint --ext .ts,.tsx,.vue .",
    "fix": "eslint --fix --ext .ts,.tsx,.vue .",
    "format": "prettier -cwu --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curegit/stylegan-vue.git"
  },
  "author": "curegit (https://github.com/curegit)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curegit/stylegan-vue/issues"
  },
  "homepage": "https://github.com/curegit/stylegan-vue",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.0"
  }
}
